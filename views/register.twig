{% extends "layout.twig" %}


{% block main %}
    <h3>Login</h3>
    <form method="POST" action="/register" role="form" class="form-horizontal">
        {{ my_form | safe }}
        <div class="form-group">
            <div class="col-sm-offset-2 col-sm-10">
                <button type="submit" class="btn btn-default pull-right">Register</button>
            </div>
        </div>
    </form>
{% endblock %}

